{
  "name": "quiqflow-training",
  "version": "1.0.0",
  "description": "QuiqFlow Training Exercises",
  "main": "dist/script.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/Week-2/Notes-API/index.js",
    "express-api": "tsx src/Week-2/Notes-API/index.ts",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts}\" --fix",
    "format": "prettier --write \"src/**/*.{js,ts}\"",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.3",
    "typescript": "^5.0.0"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  }
}
