{
  "name": "quiqflow-training",
  "version": "1.0.0",
  "description": "QuiqFlow Training Exercises",
  "main": "dist/script.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/Week-2/BE-Day-2-Express-TypeScript/index.js",
    "start:js": "node src/js/script.js",
    "dev:ts": "node --loader ts-node/esm src/ts/script.ts",
    "dev:js": "node src/js/script.js",
    "migrate": "node scripts/migrate-js-to-ts.js",
    "dev": "ts-node --esm src/Week-2/BE-Day-2-Express-TypeScript/index.ts",
    "express-api": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" node src/Week-2/BE-Day-2-Express-TypeScript/index.ts"
  },
  "dependencies": {
    "express": "^5.1.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^20.0.0",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  }
}
